{
    "name":         "supervisord/supervisor-bundle",
    "description":  "Supervisor service with Symfony2 commands ",
    "keywords":     ["supervisor","supervisord"],
    "homepage":     "https://github.com/smatejic/SupervisordBundle",
    "type":         "symfony-bundle",
    "license":      "Apache2",
    "authors": [
        {
            "name":   "Sasha Matejic",
            "email":  "smatejic@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.1,<3.0-dev"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Required when using the webinterface.",
        "symfony/twig-bundle": "Required when using the webinterface."
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Supervisord\\SupervisorBundle": "" }
    },
    "target-dir": "Supervisord/SupervisorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
